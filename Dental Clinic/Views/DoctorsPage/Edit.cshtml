@model Dental_Clinic.Models.EntryFormModel
@{
    ViewData["Title"] = "Edit Page";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-center">Edit</h1>

<br />
<br />

<form href="#" method="post">
    <div class="bd">
        <div align="center">
            <div>
                Choose a procedure :
                <select asp-for="Procedure" value="@Model.Procedure">
                    @foreach (var item in (List<ProcedureModel>)ViewData["ListOfProcedures"])
                    {
                        <option value=@item.Name>@item.Name</option>
                    }
                </select>
                <a asp-action="Index" asp-controller="ListOfProcedures">
                    <img width="15" height="15" src="data:image/png;base64," alt="" />
                </a>

            </div>
            <br />
            <div>
                Choose a doctor :
                <select asp-for="Doctor" value="@Model.Doctor">
                    @foreach (var item in (List<EmployeeModel>)ViewData["ListOfDoctors"])
                    {
                        <option value=@item.Name>@item.Name</option>
                    }
                </select>

                <a asp-action="Index" asp-controller="ListOfDoctors">
                    <img width="15" height="15" src="data:image/png;base64," alt="" />
                </a>
            </div>
            <br />
            <div>
                Check doctor availability :
                <input type="datetime-local" asp-for="StartOfProcedure" value="@((string)ViewData["Date-Time"])" />
                <span asp-validation-for="StartOfProcedure" class="text-danger"></span>

                @((string)ViewData["Date-Time"])
                @if ((string)ViewData["Time_problem_message"] != null)
                {
                    <div class="text-danger">@ViewData["Time_problem_message"]</div>
                }

            </div>

            <br />

            <hr width=85% size="5" color="black" />

            <br />

            <div>
                Your name:
                <input type="text" asp-for="UserName" value="@Model.UserName" placeholder="name" />

                <div>
                    <span asp-validation-for="UserName" class="text-danger"></span>
                </div>

            </div>
            <br />
            <div>
                Your Phone number:
                <input type="tel" asp-for="UserPhoneNumber" value="@Model.UserPhoneNumber" placeholder="number of phone" />

                <div>
                    <span asp-validation-for="UserPhoneNumber" class="text-danger"></span>
                </div>

            </div>
            <br />
            <div>

                Your Email:
                <input type="email" asp-for="UserEmail" value="@Model.UserEmail" placeholder="email" />

                <div>
                    <span asp-validation-for="UserEmail" class="text-danger"></span>
                </div>

            </div>
            <br />
            <div>
                <textarea asp-for="Comment" value="@Model?.Comment" placeholder="here comments"></textarea>
            </div>
        </div>
        <br />

        <div class="text-center">
            <button type="submit">Edit</button>
        </div>

        <br />
    </div>
</form>
<script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.5.1.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validate/1.17.0/jquery.validate.min.js"></script>
    <script src="https://ajax.aspnetcdn.com/ajax/jquery.validation.unobtrusive/3.2.10/jquery.validate.unobtrusive.min.js"></script>

